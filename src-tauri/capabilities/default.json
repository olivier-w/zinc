{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "core:event:default",
    "core:event:allow-emit",
    "core:event:allow-listen",
    "core:window:allow-set-progress-bar"
  ]
}
